                        MetaInfo:
                            id: exif_file_size
                            key: "File Size"
                        MetaInfo:
                            id: exif_create_date
                            key: "Create Date"
                        MetaInfo:
                            id: exif_image_size
                            key: "Image Size"
                        MetaInfo:
                            id: exif_iso_speed
                            key: "ISO Speed"
                        MetaInfo:
                            id: exif_aperture
                            key: "Aperture"
                        MetaInfo:
                            id: exif_shutter_speed
